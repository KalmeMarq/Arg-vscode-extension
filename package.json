{
  "name": "alg",
  "displayName": "VisualG",
  "publisher": "KalmeMarq",
  "description": "Adds syntax highlight and some snippets for visualg.",
  "icon": "visualg.png",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "alg",
        "aliases": ["alg", "alg"],
        "extensions": ["alg"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "alg",
        "scopeName": "source.alg",
        "path": "./syntaxes/alg.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "alg",
        "path": "./snippets.json"
      }
    ]
  }
}